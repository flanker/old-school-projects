            <div class="clear"></div>
        </div>
        <div id="content">
            <asp:ContentPlaceHolder ID="InPageContent" runat="server" />
        </div>
        <div id="footer" class="fullWidthLine">
            <% Html.RenderPartial("FooterDiv"); %>
